{
  "name": "intraアシスタント",
  "version": "1.03",
  "description": "スケジュール追加／更新する時に参加メンバーの選択を容易にする。",
  "permissions": [
    "storage",
    "https://www.tci-cn.co.jp/intra/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.tci-cn.co.jp/intra/",
        "https://www.tci-cn.co.jp/intra/modules/fischher/weekly.php*",
        "https://www.tci-cn.co.jp/intra/modules/fischher/regist.php*", 
        "https://www.tci-cn.co.jp/intra/modules/fischher/update.php*"],

      "css": ["jquery-ui-1.12.1.custom/jquery-ui.min.css", "css.css"],
      "js": ["jquery-3.3.1.min.js", "jquery-ui-1.12.1.custom/jquery-ui.min.js", "background.js"]
    }
  ],
  "web_accessible_resources": ["jquery-ui-1.12.1.custom/images/*"],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
